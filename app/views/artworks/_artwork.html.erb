<div class="col-12 col-sm-6 col-md-4 col-lg-4 mb-4">
  <div class="card">
    <%= image_tag artwork.image, class: "card-img-top", alt: artwork.title if artwork.image.attached? %>
    <div class="card-body">
      <p class="card-text"><%= artwork.title %></p>
      <p class="card-text"><%= artwork.medium %></p>
    </div>
  </div>
</div>
